[core]
	excludesfile = /Users/albert/.gitignore
	ignorecase = false
[color]
	status = auto
	diff = auto
	branch = auto
[branch "master"]
	merge = master
[alias]
	who = shortlog -s -n --no-merges
	standup = shortlog --since=yesterday
	today = shortlog --since=midnight
	uncommit = reset --soft HEAD^
	unref = reset --soft HEAD@{1}
[github]
	user = soma
	token = <.replace github-token>
[push]
	default = current
[web]
	browser = open
